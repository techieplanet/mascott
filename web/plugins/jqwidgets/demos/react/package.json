{
  "name": "jqwidgets-react-webpack",
  "version": "0.0.1",
  "description": "jqwidgets-react-webpack",
  "dependencies": {
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "webpack": "2.4.1",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1"
  },
  "watch": {
    "start": {
      "patterns": [ "./app" ],
      "quiet": true
    }
  },
    "scripts": {
        "start": "npm run build1 && npm run build2 && npm run build3 && npm run build4 && npm run build5 && npm run build6",
        "build1": "webpack --config webpack.config1.js",
        "build2": "webpack --config webpack.config2.js",
        "build3": "webpack --config webpack.config3.js",
        "build4": "webpack --config webpack.config4.js",
        "build5": "webpack --config webpack.config5.js",
        "build6": "webpack --config webpack.config6.js",
        "watch": "npm-watch"
    },
  "author": "www.jQWidgets.com"
}
